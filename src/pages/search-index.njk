---
data: artist
layout: json
permalink: "/search-index.json"
eleventyExcludeFromCollections: true
---
[
  {% for index in artist %}
    {
      "title": "{{ index.title }}",
      "date": "{{ index.date }}",
      "location": "{{ index.location[0].name }}",
      {% if index.content %}
        "content": "{{ index.content | stripEverything }}",
      {% else %}
        "content": "",
      {% endif %}
      "url": "/painting-date/{{ index.date | slug }}-{{ index.title | slug }}/"
    }{% if not loop.last %},{% endif %}
  {% endfor %}
]
